  "scripts": {
    "clean": "rimraf build",
    "start": "concurrently \"yarn start:docs\" \"yarn start:webpack\"",
    "start:docs": "docusaurus-start",
    "start:webpack": "node scripts/copy-packs.js && webpack -w -d",
    "start:webpack-old": "webpack-dev-server -d --content-base ./static/",
    "build": "yarn packs && webpack -p && docusaurus-build",
    "packs": "rimraf ./static/packs && node scripts/copy-packs.js",
    "publish-gh-pages": "yarn build && USE_SSH=true docusaurus-publish",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version"
  },
  "dependencies": {
    "@phensley/cldr": "^0.6.3",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.4",
    "emotion": "^9.2.4",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.4",
    "react-redux": "^5.0.7",
    "rebass": "^2.0.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.3.3",
    "wretch": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "8.10.20",
    "concurrently": "^3.6.0",
    "docusaurus": "^1.3.0",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml": "^3.12.0",
    "ts-loader": "^4.4.1",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
