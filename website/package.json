  "version": "0.8.17",
  "scripts": {
    "clean": "rimraf build",
    "deploy": "yarn build && USE_SSH=true docusaurus-publish",
    "start": "node gencrossref.js && concurrently \"yarn start:docs\" \"yarn start:webpack\"",
    "start:docs": "docusaurus-start",
    "start:webpack": "node scripts/copy-packs.js && webpack-dev-server -w -d",
    "start:webpack-old": "webpack-dev-server -d --content-base ./static/",
    "build": "node gencrossref.js && yarn packs && webpack -p && docusaurus-build",
    "packs": "rimraf ./static/packs && node scripts/copy-packs.js",
    "report": "source-map-explorer --html -m ./build/cldr-engine/liveapi/liveapi.js >report.html",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version"
  },
  "dependencies": {
    "@phensley/cldr": "0.19.3",
    "@types/react-router-dom": "4.3.5",
    "emotion": "9.2.4",
    "icepick": "2.3.0",
    "prop-types": "15.6.2",
    "pure-store": "0.3.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-emotion": "9.2.4",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-saga": "1.1.1",
    "styled-components": "3.3.3",
    "tslib": "1.10.x",
    "wretch": "1.3.1"
  },
  "devDependencies": {
    "@types/deasync": "^0.1.0",
    "@types/icepick": "^1.3.0",
    "@types/node": "10.14.5",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^6.0.3",
    "@types/yargs": "^13.0.2",
    "concurrently": "^3.6.0",
    "deasync": "^0.1.15",
    "docusaurus": "1.13.0",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml": "^3.12.0",
    "source-map-explorer": "^1.8.0",
    "ts-lint": "^4.5.1",
    "ts-loader": "^4.4.1",
    "ts-node": "^7.0.0",
    "tslint-no-circular-imports": "^0.2.1",
    "typescript": "*",
    "typescript-styled-plugin": "^0.13.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "yargs": "^14.0.0"
  }
}
