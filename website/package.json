{
  "private": true,
  "author": "Patrick Hensley <pathensley@gmail.com>",
  "license": "Apache-2.0",
  "version": "1.5.1",
  "scripts": {
    "clean": "rimraf build",
    "deploy": "yarn build && USE_SSH=true docusaurus-publish",
    "generate": "ts-node src/docgen/index.ts",
    "start": "rimraf build && node gencrossref.js && concurrently \"yarn start:docs\" \"yarn start:webpack\"",
    "start:docs": "docusaurus-start --no-watch",
    "start:webpack": "node scripts/copy-packs.js && webpack serve --mode development",
    "start:webpack-old": "webpack-dev-server -d --content-base ./static/",
    "build": "node gencrossref.js && yarn packs && docusaurus-build",
    "build:wp": "node gencrossref.js && yarn packs && webpack -p && docusaurus-build",
    "packs": "rimraf ./static/packs && node scripts/copy-packs.js",
    "report": "source-map-explorer --html -m ./build/cldr-engine/liveapi/liveapi.js >report.html",
    "write-translations": "docusaurus-write-translations",
    "new-version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version",
    "link-all": "ls --color=none node_modules/@phensley | xargs -I % yarn link @phensley/%",
    "unlink-all": "ls --color=none node_modules/@phensley | xargs -I % yarn unlink @phensley/%"
  },
  "dependencies": {
    "@phensley/cldr": "1.7.3",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "emotion": "11.0.0",
    "icepick": "2.4.0",
    "prop-types": "15.8.1",
    "pure-store": "1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-emotion": "10.0.0",
    "react-redux": "8.1.1",
    "react-router-dom": "6.14.1",
    "redux": "4.2.1",
    "redux-saga": "1.2.3",
    "styled-components": "6.0.2",
    "tslib": "2.6.x",
    "wretch": "2.6.0"
  },
  "devDependencies": {
    "@types/deasync": "^0.1.2",
    "@types/icepick": "^2.3.0",
    "@types/node": "20.4.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-redux": "^7.1.25",
    "@types/yargs": "^17.0.24",
    "concurrently": "^8.2.0",
    "deasync": "^0.1.28",
    "docusaurus": "1.14.7",
    "html-webpack-plugin": "^5.5.3",
    "js-yaml": "^4.1.0",
    "source-map-explorer": "^2.5.3",
    "ts-lint": "^4.5.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "5.1",
    "typescript-styled-plugin": "^0.18.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "yargs": "^17.7.2"
  }
}
